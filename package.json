{
	"name": "event-discovery-hub",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env NODE_ENV=development node server/index.js",
		"build": "vite build && npm run copy-spa-fallback",
		"build:windows": "vite build && powershell -Command \"Copy-Item dist\\public\\index.html dist\\public\\200.html\"",
		"build:linux": "vite build && cp dist/public/index.html dist/public/200.html",
		"copy-spa-fallback": "node -e \"const fs=require('fs'); fs.copyFileSync('dist/public/index.html', 'dist/public/200.html')\"",
		"start": "cross-env NODE_ENV=production node server/index.js",
		"db:push": "drizzle-kit push",
		"setup-check": "node setup-check.js",
		"setup": "node quick-setup.js",
		"postinstall": "npm run setup-check",
		"clean": "rimraf node_modules package-lock.json && npm install",
		"audit-fix": "npm audit fix --force",
		"lint": "eslint client/src --ext .js,.jsx",
		"lint:fix": "eslint client/src --ext .js,.jsx --fix",
		"format": "prettier --write \"client/src/**/*.{js,jsx,css,json}\"",
		"type-check": "echo 'Type checking skipped - using JavaScript'",
		"preview": "vite preview --host",
		"deploy": "npm run build && firebase deploy",
		"deploy:surge": "npm run build && cd dist/public && surge . event-discovery-hub.surge.sh"
	},
